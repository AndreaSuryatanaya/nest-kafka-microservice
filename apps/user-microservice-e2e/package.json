{
  "name": "@org/user-microservice-e2e",
  "version": "0.0.1",
  "private": true,
  "nx": {
    "name": "user-microservice-e2e",
    "implicitDependencies": [
      "user-microservice"
    ],
    "targets": {
      "e2e": {
        "executor": "@nx/jest:jest",
        "outputs": [
          "{projectRoot}/test-output/jest/coverage"
        ],
        "options": {
          "jestConfig": "apps/user-microservice-e2e/jest.config.cts",
          "passWithNoTests": true
        },
        "dependsOn": [
          "user-microservice:build",
          "user-microservice:serve"
        ]
      }
    }
  }
}
